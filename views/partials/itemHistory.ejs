<div class="app-wrapper">

    <div class="app-content pt-3 p-md-3 p-lg-4">
        <div class="container-xl">

            <div class="col-auto mt-100">
                <h1 class="app-page-title mb-100">
                    <%= itemName %>
                </h1>
            </div>
            <div class="row g-3 mb-4 align-items-center justify-content-between">
                <form class="form-group">
                    <div class="row">
                        <div class="col-12 col-md-8" id="filterBns"></div>
                        <div class="col-12 col-md-4">
                            <div class="d-flex  justify-content-center justify-content-md-end align-items-center">
                                <label for="searchField">Search:</label>
                                <input type="text" class="form-control" id="searchField" style="max-width: 250px;">
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="tab-content" id="orders-table-tab-content">
                <div class="tab-pane fade show active" id="ih-iv" role="tabpanel" aria-labelledby="ih-iv-tab">
                    <div class="app-card app-card-orders-table shadow-sm mb-5">
                        <div class="app-card-body">
                            <div class="table-responsive">
                                <table id="itemHistory" class="table  app-table-hover mb-0" cellspacing="0"
                                    width="100%">
                                    <thead>
                                        <tr>
                                            <th class="cell">Voucher ID</th>
                                            <th class="cell">Station / Supplier</th>
                                            <th class="cell">Quantity</th>
                                            <th class="cell">
                                                ApprovalDate
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% if (itemHistoryIVResult.length < 1 ) { %>
                                            <% } else { %>
                                                <% itemHistoryIVResult.forEach( function(item) { %>
                                                    <tr>
                                                        <td class="cell">
                                                            IV/<%= item.IVNo %>/<%= item.IVYear %>
                                                        </td>
                                                        <td class="cell">
                                                            <%= item.stationName %>
                                                        </td>
                                                        <td class="cell">
                                                            - <%= item.ivQtyPassed %>
                                                        </td>
                                                        <td class="cell">
                                                            <%= new Date(item.ApprovalDate).toISOString().slice(0, 10)
                                                                + " " + new
                                                                Date(item.ApprovalDate).toISOString().slice(11, 19) %>
                                                        </td>
                                                    </tr>
                                                    <% }) %>
                                                        <% itemHistoryRVResult.forEach( function(item) { %>
                                                            <tr>
                                                                <td class="cell">
                                                                    RV/<%= item.RVNo %>/<%= item.RVYear %>
                                                                </td>
                                                                <td class="cell">
                                                                    <%= item.Supplier %>
                                                                </td>
                                                                <td class="cell">
                                                                    + <%= item.rvItemQty %>
                                                                </td>
                                                                <td class="cell">
                                                                    <%= new
                                                                        Date(item.ApprovalDate).toISOString().slice(0,
                                                                        10) + " " + new
                                                                        Date(item.ApprovalDate).toISOString().slice(11,
                                                                        19) %>
                                                                </td>
                                                            </tr>
                                                            <% }) %>
                                                                <% } %>

                                    </tbody>
                                </table>
                            </div>
                            <!--//table-responsive-->

                        </div>
                        <!--//app-card-body-->
                    </div>
                    <!--//app-card-->


                </div>


            </div>
            <!--//container-fluid-->
        </div>
        <!--//app-content-->

        <footer class="app-footer">
            <div class="container text-center py-3">
                <small class="copyright">Designed with <span class="sr-only">love</span><i class="fas fa-heart"
                        style="color: #fb866a;"></i> by students of<a class="app-link"
                        href="https://www.psgtech.edu/hme.php?var=AMC" target="_blank"> PSG TECH</a> for Coimbatore City
                    Police Commissioner Office</small>

            </div>
        </footer>
        <!--//app-footer-->

    </div>