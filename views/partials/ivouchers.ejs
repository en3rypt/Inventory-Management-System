                                            <th class="cell">DateOfReceival</th>
                                            <th class="cell">Approval</th>
                                            <th class="cell">ActionBy</th>
                                            <th class="cell">ActionDate</th>
                                            <th class="cell"></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% ivouchersData.forEach(function(issue) { %>
                                            <% if (issue.Approval==2 ) { %>
                                                <tr>
                                                    <td class="cell">
                                                        IV/<%= issue.IVNo %>/<%= issue.IVYear %>
                                                    </td>
                                                    <td class="cell">
                                                        <%= issue.Receiver %>
                                                    </td>
                                                    <td class="cell">
                                                        <%= issue.SNo %>
                                                    </td>
                                                    <td class="cell">
                                                        <%= issue.Scheme %>
                                                    </td>
                                                    <td class="cell">
                                                        <%= new Date(issue.DateOfReceival).toISOString().slice(0, 10) %>
                                                    </td>
                                                    <td class="cell">
                                                        <% if (issue.Approval==0 ) { %>
                                                            <span class="badge badge bg-warning">Pending</span>
                                                            <% } else if (issue.Approval==1 ) { %>
                                                                <span class="badge badge bg-success">Approved</span>
                                                                <% } else if (issue.Approval==2 ) { %>
                                                                    <span class="badge badge bg-danger">Declined</span>
                                                                    <% } %>
                                                    </td>
                                                    <td class="cell">
                                                        <% if (issue.Approval!=0 ) { %>
                                                            <%=issue.ApprovedBy %>
                                                                <% } else { %>
                                                                    -
                                                                    <% } %>
                                                    </td>
                                                    <td class="cell">
                                                        <% if (issue.Approval!=0 ) { %>
                                                            <%= new Date(issue.ApprovalDate).toISOString().slice(0, 10)
                                                                + " " + new
                                                                Date(issue.ApprovalDate).toISOString().slice(11, 19) %>
                                                                <% } else { %>
                                                                    -
                                                                    <% } %>
                                                    </td>
                                                    <td class="cell">

                                                        <div class="col-auto">
                                                            <button type="button" class="btn app-btn-secondary"
                                                                data-bs-toggle="modal"
                                                                data-bs-target="#v<%=issue.ID%>">View</button>
                                                            <div class="modal fade" id="v<%=issue.ID%>" tabindex="-1"
                                                                aria-labelledby="v<%=issue.ID%>Label"
                                                                aria-hidden="true">
                                                                <div
                                                                    class="modal-dialog modal-xl modal-dialog-scrollable">
                                                                    <div class="modal-content">
                                                                        <div class="modal-header">
                                                                            <h5 class="modal-title"
                                                                                id="viewRequewstLabel">
                                                                                Issued Items List
                                                                            </h5>
                                                                            <button type="button" class="btn-close"
                                                                                data-bs-dismiss="modal"
                                                                                aria-label="Close"></button>
                                                                        </div>
                                                                        <div class="modal-body">
                                                                            <table class="table  table-hover">
                                                                                <thead>
                                                                                    <tr>
                                                                                        <th scope="col">Item</th>
                                                                                        <th scope="col">Requested</th>
                                                                                        <th scope="col">Passed</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody>
                                                                                    <% Object.keys(ivItemlist).forEach(function(rID)
                                                                                        { %>
                                                                                        <% if (rID==issue.ID) { %>
                                                                                            <% Object.keys(ivItemlist[rID]).forEach(function(item)
                                                                                                { %>

                                                                                                <tr>
                                                                                                    <td>
                                                                                                        <%= item %>
                                                                                                    </td>
                                                                                                    <td>
                                                                                                        <%= ivItemlist[rID][item].req
                                                                                                            %>
                                                                                                    </td>
                                                                                                    <td>
                                                                                                        <%= ivItemlist[rID][item].passed
                                                                                                            %>
                                                                                                    </td>
                                                                                                </tr>

                                                                                                <% }) %>
                                                                                                    <% } %>
                                                                                                        <% }) %>
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                        <% if(auth==2){ %>
                                                                            <div class="modal-footer">

                                                                                <% if (issue.Approval !=1 &&
                                                                                    issue.Approval !=2) { %>
                                                                                    <form
                                                                                        action="/ivouchers/action/<%=issue.ID%>/<%=id%>"
                                                                                        method="post" name="ratings">
                                                                                        <!-- <input type="hidden" name="Id"
                                                                                value=""> -->
                                                                                        <button type="submit"
                                                                                            name="action_type"
                                                                                            value="Accept"
                                                                                            class="btn btn-success text-white">Approve</button>
                                                                                        <button type="submit"
                                                                                            name="action_type"
                                                                                            value="Decline"
                                                                                            class="btn btn-danger text-white"
                                                                                            data-bs-dismiss="modal">Decline</button>
                                                                                    </form>
                                                                                    <% } %>
                                                                            </div>
                                                                            <% } %>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <% } %>
                                                    <% }); %>
                                    </tbody>
                                </table>
                            </div>
                            <!--//table-responsive-->

                        </div>
                        <!--//app-card-body-->
                    </div>
                    <!--//app-card-->
                </div>
                <!--//tab-pane-->
            </div>
            <!--//tab-content-->



        </div>
        <!--//container-fluid-->
    </div>
    <!--//app-content-->

    <footer class="app-footer">

    </footer>
    <!--//app-footer-->
</div>